{
  "kind": "singleType",
  "collectionName": "welcome_texts",
  "info": {
    "singularName": "welcome-text",
    "pluralName": "welcome-texts",
    "displayName": "Welcome Text"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Heading": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "Welcome"
    },
    "MainContent": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true,
      "default": "TCD Sense - The Trinity Sensory Processing Project - aims to make Trinity more inclusive by reviewing and improving new and existing spaces, building sensory awareness and delivering specialist supports to students who experience barriers to managing and adapting the sensory environments of college."
    },
    "CloseButtonText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "Close"
    },
    "LinkText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false,
      "default": "About TCDSense"
    },
    "ExternalButton": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "Link": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    }
  }
}
